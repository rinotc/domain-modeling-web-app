{
  "family": "sdmt-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sdmt-web-container",
      "image": "sdmt",
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "runtimePlatform": {
    "cpuArchitecture": "ARM",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::aws:policy/service-role/AmazonECSTaskExecutionRolePolicy",
  "cpu": "512",
  "memory": "1024"
}